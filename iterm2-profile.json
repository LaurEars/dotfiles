{
  "Profiles": [{
    "Name": "Dotfiles",
    "Guid": "dotfiles-default-profile",
    "Badge Text": "\\(user.gitRepo)",
    "ClickToSelectCommand": false,
    "Smart Selection Rules": [
      {
        "notes": "file_path:line_no",
        "precision": "very_low",
        "regex": "[\\w/]+\\.\\w+:\\d+"
      },
      {
        "notes": "git diff file path",
        "precision": "high",
        "regex": "(?<=[ab]\\\\/)([\\\\w/]+\\\\.\\\\w+)"
      }
    ]
  }]
}
